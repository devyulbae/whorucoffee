{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python@3.1.0"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
} 